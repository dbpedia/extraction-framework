                        </parameters> 
                        <wikiTemplate>{{-homo-}}
(* $line
)+
</wikiTemplate>
                    </template>
                    <template name="anagram" ppClass="org.dbpedia.extraction.mappings.wikitemplate.wiktionary.bindinghandler.LinkListHelper">
