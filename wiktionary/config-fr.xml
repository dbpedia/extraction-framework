                        </parameters> 
                        <wikiTemplate>{{-var-ortho-}}
(* $line
)+
</wikiTemplate>
                    </template>
                    <template name="homonym" ppClass="org.dbpedia.extraction.mappings.wikitemplate.wiktionary.bindinghandler.LinkListHelper">
