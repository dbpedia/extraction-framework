                        <resultTemplates>
                            <resultTemplate>
                                <triples>
                                    <triple s="$block" p="http://wiktionary.dbpedia.org/terms/hasProverb" o="$proverb" oType="literal" />
                                </triples>
                            </resultTemplate>
                        </resultTemplates>                    
                        <wikiTemplate>{{Sprichwörter}}
(:$proverb
)+
