@base	          	<http://uk.dbpedia.org/resource/Mapping_uk:Infobox_Constellation/> .
@prefix schema: 	<http://schema.org/> .
@prefix rr:    		<http://www.w3.org/ns/r2rml#> .
@prefix owl:   		<http://www.w3.org/2002/07/owl#> .
@prefix gml:   		<http://www.opengis.net/gml/> .
@prefix xsd:   		<http://www.w3.org/2001/XMLSchema#> .
@prefix bio:   		<http://purl.org/vocab/bio/0.1/> .
@prefix skos:  		<http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix d0:    		<http://www.ontologydesignpatterns.org/ont/d0.owl#> .
@prefix geo:   		<http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rml:   		<http://semweb.mmlab.be/ns/rml#> .
@prefix dct:   		<http://purl.org/dc/terms/> .
@prefix fnml:  		<http://semweb.mmlab.be/ns/fnml#> .
@prefix dcterms: 	<http://purl.org/dc/terms/> .
@prefix ql:    		<http://semweb.mmlab.be/ns/ql#> .
@prefix dbr-en: 	<http://en.dbpedia.org/resource/> .
@prefix foaf:  		<http://xmlns.com/foaf/0.1/> .
@prefix crml:  		<http://semweb.mmlab.be/ns/crml#> .
@prefix fno:   		<http://w3id.org/function/ontology#> .
@prefix georss: 	<http://www.georss.org/georss/> .
@prefix dbf:   		<http://dbpedia.org/function/> .
@prefix dbo:   		<http://dbpedia.org/ontology/> .
@prefix mappings: 	<http://mappings.dbpedia.org/wiki/> .
@prefix rdf:   		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix bibo:  		<http://purl.org/ontology/bibo/> .
@prefix dul:   		<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix cidoccrm: 	<http://purl.org/NET/cidoc-crm/core#> .
@prefix wikidata: 	<http://www.wikidata.org/entity/> .
@prefix dc:    		<http://purl.org/dc/elements/1.1/> .


### Main TriplesMap
####################
<http://uk.dbpedia.org/resource/Mapping_uk:Infobox_Constellation>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/1> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     dbo:CelestialBody ,
		     schema:Place ,
		     dbo:Location ,
		     owl:Thing ,
		     dbo:Constellation ,
		     dbo:Place ,
		     wikidata:Q8928 ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://uk.wikipedia.org/wiki/Mapping_uk:Infobox_Constellation?oldid=43404" .


###########
# Mappings
###########

### Predicate Object Map
#########################
<SimplePropertyMapping/1>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/1/FTM> ;
        rr:predicate  dbo:abbreviation .

### Function Term Map
<SimplePropertyMapping/1/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <SimplePropertyMapping/1/FTM/FV> .

### Function Execution Mapping
<SimplePropertyMapping/1/FTM/FV>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <Function/SimplePropertyFunction> ,
			       <SimplePropertyMapping/1/FTM/FV/propertyParameterPOM> ;
        rr:subjectMap          <SubjectMap/Function> .

### Parameters
<SimplePropertyMapping/1/FTM/FV/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/1/FTM/FV/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

### References
<SimplePropertyMapping/1/FTM/FV/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "abbreviation" .


### Predicate Object Map
#########################
<SimplePropertyMapping/0>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/0/FTM> ;
        rr:predicate  dbo:neighbourConstellations .

### Function Term Map
<SimplePropertyMapping/0/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <SimplePropertyMapping/0/FTM/FV> .

### Function Execution Mapping
<SimplePropertyMapping/0/FTM/FV>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <Function/SimplePropertyFunction> ,
			       <SimplePropertyMapping/0/FTM/FV/propertyParameterPOM> ;
        rr:subjectMap          <SubjectMap/Function> .

### Parameters
<SimplePropertyMapping/0/FTM/FV/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/0/FTM/FV/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

### References
<SimplePropertyMapping/0/FTM/FV/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "bordering" .


######################
# Conditional Mappings
######################



############
# Functions
############


<Function/SimplePropertyFunction>
        a             rr:PredicateObjectMap ;
        rr:object     dbf:simplePropertyFunction ;
        rr:predicate  fno:executes .


### Functions SubjectMap
#########################

<SubjectMap/Function>
        a            rr:SubjectMap ;
        rr:class     fno:Execution ;
        rr:termType  rr:BlankNode .
