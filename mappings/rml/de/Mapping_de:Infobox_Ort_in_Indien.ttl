### Main TriplesMap
####################
<http://de.dbpedia.org/resource/Mapping_de:Infobox_Ort_in_Indien>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/0> ,
			       <latitude> ,
			       <SimplePropertyMapping/4> ,
			       <SimplePropertyMapping/8> ,
			       <SimplePropertyMapping/6> ,
			       <SimplePropertyMapping/2> ,
			       <longitude> ,
			       <SimplePropertyMapping/3> ,
			       <SimplePropertyMapping/7> ,
			       <SimplePropertyMapping/5> ,
			       <SimplePropertyMapping/1> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     owl:Thing ,
		     dbo:PopulatedPlace ,
		     wikidata:Q486972 ,
		     dbo:Settlement ,
		     dbo:Place ,
		     dbo:Location ,
		     schema:Place ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://de.wikipedia.org/wiki/Mapping_de:Infobox_Ort_in_Indien?oldid=31744" .


