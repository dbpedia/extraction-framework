### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:US-garden-stub/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:US-garden-stub/ConstantMapping/location/United_States> ,
			       <./Mapping_en:US-garden-stub/ConstantMapping/country/United_States> ;
        rr:subjectMap          <./Mapping_en:US-garden-stub/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:US-garden-stub/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Garden ,
		     wikidata:Q1107656 ,
		     dbo:Place ,
		     schema:Place ,
		     dbo:Location ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:US-garden-stub/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:US-garden-stub?oldid=29713" .


