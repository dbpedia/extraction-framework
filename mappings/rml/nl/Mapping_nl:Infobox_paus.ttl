### Main TriplesMap
####################
<http://nl.dbpedia.org/resource/Mapping_nl:Infobox_paus>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/5> ,
			       <SimplePropertyMapping/4> ,
			       <SimplePropertyMapping/3> ,
			       <StartDateMapping/0> ,
			       <SimplePropertyMapping/9> ,
			       <EndDateMapping/0> ,
			       <SimplePropertyMapping/8> ,
			       <SimplePropertyMapping/7> ,
			       <SimplePropertyMapping/2> ,
			       <SimplePropertyMapping/10> ,
			       <SimplePropertyMapping/1> ,
			       <SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/6> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     dul:NaturalPerson ,
		     wikidata:Q19546 ,
		     dbo:Person ,
		     dbo:Cleric ,
		     dul:Agent ,
		     owl:Thing ,
		     wikidata:Q215627 ,
		     dbo:Pope ,
		     dbo:Agent ,
		     foaf:Person ,
		     schema:Person ,
		     wikidata:Q5 ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://nl.wikipedia.org/wiki/Mapping_nl:Infobox_paus?oldid=24896" .


