@base	          	<http://pl.dbpedia.org/resource/Mapping_pl:Linki_do_map_Polski/> .
@prefix schema: 	<http://schema.org/> .
@prefix rr:    		<http://www.w3.org/ns/r2rml#> .
@prefix owl:   		<http://www.w3.org/2002/07/owl#> .
@prefix gml:   		<http://www.opengis.net/gml/> .
@prefix xsd:   		<http://www.w3.org/2001/XMLSchema#> .
@prefix bio:   		<http://purl.org/vocab/bio/0.1/> .
@prefix skos:  		<http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix d0:    		<http://www.ontologydesignpatterns.org/ont/d0.owl#> .
@prefix geo:   		<http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rml:   		<http://semweb.mmlab.be/ns/rml#> .
@prefix dct:   		<http://purl.org/dc/terms/> .
@prefix fnml:  		<http://semweb.mmlab.be/ns/fnml#> .
@prefix dcterms: 	<http://purl.org/dc/terms/> .
@prefix ql:    		<http://semweb.mmlab.be/ns/ql#> .
@prefix dbr-en: 	<http://en.dbpedia.org/resource/> .
@prefix foaf:  		<http://xmlns.com/foaf/0.1/> .
@prefix crml:  		<http://semweb.mmlab.be/ns/crml#> .
@prefix fno:   		<http://w3id.org/function/ontology#> .
@prefix georss: 	<http://www.georss.org/georss/> .
@prefix dbf:   		<http://dbpedia.org/function/> .
@prefix dbo:   		<http://dbpedia.org/ontology/> .
@prefix mappings: 	<http://mappings.dbpedia.org/wiki/> .
@prefix rdf:   		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix bibo:  		<http://purl.org/ontology/bibo/> .
@prefix dul:   		<http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix cidoccrm: 	<http://purl.org/NET/cidoc-crm/core#> .
@prefix wikidata: 	<http://www.wikidata.org/entity/> .
@prefix dc:    		<http://purl.org/dc/elements/1.1/> .


### Main TriplesMap
####################
<http://pl.dbpedia.org/resource/Mapping_pl:Linki_do_map_Polski>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <latitude> ,
			       <longitude> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     dbo:Place ,
		     schema:Place ,
		     dbo:Location ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://pl.wikipedia.org/wiki/Mapping_pl:Linki_do_map_Polski?oldid=12379" .


###########
# Mappings
###########

### Predicate Object Map
#########################
<longitude>  a        rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap> ;
        rr:predicate  geo:long .

### Function Term Map
<longitude/ObjectMap>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <longitude/ObjectMap/FV> .

### Function Execution Mapping
<longitude/ObjectMap/FV>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <Function/LongitudeFunction> ,
			       <longitude/ObjectMap/FV/lonDegreesParameterPOM> ,
			       <longitude/ObjectMap/FV/lonMinutesParameterPOM> ,
			       <longitude/ObjectMap/FV/lonSecondsParameterPOM> ,
			       <longitude/ObjectMap/FV/lonDirectionParameterPOM> ;
        rr:subjectMap          <SubjectMap/Function> .

### Parameters
<longitude/ObjectMap/FV/lonDegreesParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap/FV/lonDegreesParameterPOM/ObjectMap> ;
        rr:predicate  dbf:lonDegreesParameter .

<longitude/ObjectMap/FV/lonDirectionParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap/FV/lonDirectionParameterPOM/ObjectMap> ;
        rr:predicate  dbf:lonDirectionParameter .

<longitude/ObjectMap/FV/lonSecondsParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap/FV/lonSecondsParameterPOM/ObjectMap> ;
        rr:predicate  dbf:lonSecondsParameter .

<longitude/ObjectMap/FV/lonMinutesParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap/FV/lonMinutesParameterPOM/ObjectMap> ;
        rr:predicate  dbf:lonMinutesParameter .

### References
<longitude/ObjectMap/FV/lonMinutesParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "minutN" .

<longitude/ObjectMap/FV/lonSecondsParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "sekundN" .

<longitude/ObjectMap/FV/lonDegreesParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "stopniN" .

<longitude/ObjectMap/FV/lonDirectionParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "null" .


### Predicate Object Map
#########################
<latitude>  a         rr:PredicateObjectMap ;
        rr:objectMap  <latitude/ObjectMap> ;
        rr:predicate  geo:lat .

### Function Term Map
<latitude/ObjectMap>  a     fnml:FunctionTermMap ;
        fnml:functionValue  <latitude/ObjectMap/FV> .

### Function Execution Mapping
<latitude/ObjectMap/FV>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <Function/LatitudeFunction> ,
			       <latitude/ObjectMap/FV/LatDegreesParameterPOM> ,
			       <latitude/ObjectMap/FV/LatMinutesParameterPOM> ,
			       <latitude/ObjectMap/FV/LatSecondsParameterPOM> ,
			       <latitude/ObjectMap/FV/latDirectionParameterPOM> ;
        rr:subjectMap          <SubjectMap/Function> .

### Parameters
<latitude/ObjectMap/FV/LatMinutesParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <latitude/ObjectMap/FV/LatMinutesParameterPOM/ObjectMap> ;
        rr:predicate  dbf:latMinutesParameter .

<latitude/ObjectMap/FV/LatDegreesParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <latitude/ObjectMap/FV/LatDegreesParameterPOM/ObjectMap> ;
        rr:predicate  dbf:latDegreesParameter .

<latitude/ObjectMap/FV/LatSecondsParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <latitude/ObjectMap/FV/LatSecondsParameterPOM/ObjectMap> ;
        rr:predicate  dbf:latSecondsParameter .

<latitude/ObjectMap/FV/latDirectionParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <latitude/ObjectMap/FV/latDirectionParameterPOM/ObjectMap> ;
        rr:predicate  dbf:latDirectionParameter .

### References
<latitude/ObjectMap/FV/LatDegreesParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "stopniE" .

<latitude/ObjectMap/FV/LatSecondsParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "sekundE" .

<latitude/ObjectMap/FV/LatMinutesParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "minutE" .

<latitude/ObjectMap/FV/latDirectionParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "null" .


######################
# Conditional Mappings
######################



############
# Functions
############


<Function/LongitudeFunction>
        a             rr:PredicateObjectMap ;
        rr:object     dbf:lonFunction ;
        rr:predicate  fno:executes .

<Function/LatitudeFunction>
        a             rr:PredicateObjectMap ;
        rr:object     dbf:latFunction ;
        rr:predicate  fno:executes .


### Functions SubjectMap
#########################

<SubjectMap/Function>
        a            rr:SubjectMap ;
        rr:class     fno:Execution ;
        rr:termType  rr:BlankNode .
