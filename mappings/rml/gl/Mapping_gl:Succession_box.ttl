### Main TriplesMap
####################
<http://gl.dbpedia.org/resource/Mapping_gl:Succession_box>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <StartDateMapping/0> ,
			       <EndDateMapping/0> ,
			       <SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/1> ,
			       <SimplePropertyMapping/2> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://gl.wikipedia.org/wiki/Mapping_gl:Succession_box?oldid=48932" .


